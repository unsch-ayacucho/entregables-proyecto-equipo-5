<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.pe.unsch.entity.SolicitanteEntity" table="solicitante" schema="cer-db">
        <id name="idSolicitante" column="id_solicitante"/>
        <property name="nombres" column="nombres"/>
        <property name="apellidoPaterno" column="apellido_paterno"/>
        <property name="apellidoMaterno" column="apellido_materno"/>
        <property name="codigo" column="codigo"/>
        <property name="dni" column="dni"/>
        <property name="direccion" column="direccion"/>
    </class>
</hibernate-mapping>