<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.pe.unsch.entity.DestinatarioEntity" table="destinatario" schema="cer-db">
        <id name="idDestinatario" column="id_destinatario"/>
        <property name="nombres" column="nombres"/>
        <property name="apellidoPaterno" column="apellido_paterno"/>
        <property name="apellidoMaterno" column="apellido_materno"/>
        <property name="cargo" column="cargo"/>
    </class>
</hibernate-mapping>