<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.pe.unsch.entity.DatoadicionalEntity" table="datoadicional" schema="cer-db">
        <id name="idDatoAdicional" column="id_dato_adicional"/>
        <property name="tipoDato" column="tipo_dato"/>
        <property name="datoAdicional" column="dato_adicional"/>
        <property name="descripcionOpcional" column="descripcion_opcional"/>
    </class>
</hibernate-mapping>