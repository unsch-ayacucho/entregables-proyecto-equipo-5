<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.pe.unsch.entity.ResolucionEntity" table="resolucion" schema="cer-db">
        <id name="idResolucion" column="id_resolucion"/>
        <property name="idTipoResolucion" column="id_tipo_resolucion"/>
        <property name="codigoResolucion" column="codigo_resolucion"/>
        <property name="codFacultad" column="cod_facultad"/>
    </class>
</hibernate-mapping>