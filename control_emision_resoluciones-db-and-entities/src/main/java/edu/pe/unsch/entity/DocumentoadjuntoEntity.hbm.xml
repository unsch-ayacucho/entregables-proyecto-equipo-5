<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.pe.unsch.entity.DocumentoadjuntoEntity" table="documentoadjunto" schema="cer-db">
        <id name="idDocumentoAdjunto" column="id_documento_adjunto"/>
        <property name="codigoDocumentoAdjunto" column="codigo_documento_adjunto"/>
        <property name="detalleContenido" column="detalle_contenido"/>
        <property name="fechaEmision" column="fecha_emision"/>
        <property name="archivoAdjunto" column="archivo_adjunto"/>
        <property name="idDatoAdicional" column="id_dato_adicional"/>
    </class>
</hibernate-mapping>