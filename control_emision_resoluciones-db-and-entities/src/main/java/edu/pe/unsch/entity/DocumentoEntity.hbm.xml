<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.pe.unsch.entity.DocumentoEntity" table="documento" schema="cer-db">
        <id name="idDocumento" column="id_documento"/>
        <property name="idTipoDocumento" column="id_tipo_documento"/>
        <property name="idSolicitante" column="id_solicitante"/>
        <property name="idDestinatario" column="id_destinatario"/>
        <property name="idResolucion" column="id_resolucion"/>
        <property name="codigoSeguimiento" column="codigo_seguimiento"/>
    </class>
</hibernate-mapping>